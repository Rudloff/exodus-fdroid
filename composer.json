{
  "name": "rudloff/exodus-fdroid",
  "description": "CLI tool to scan F-Droid packages with Îµxodus",
  "license": "GPL-3.0-only",
  "type": "project",
  "require": {
    "fdroid/php-fdroid": "dev-master",
    "exodus-privacy/exodus-standalone": "dev-master",
    "exodus-privacy/exodus-core": "~2.0.2",
    "symfony/process": "~3.4.1",
    "guzzlehttp/guzzle": "~6.3.0",
    "symfony/console": "~3.4.0",
    "androguard/androguard": "~3.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.2"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "exodus-privacy/exodus-standalone",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/Exodus-Privacy/exodus-standalone.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "exodus-privacy/exodus-core",
        "version": "2.0.2",
        "dist": {
          "url": "https://github.com/Exodus-Privacy/exodus-core/releases/download/v2.0.2/exodus_core-2.0.2.tar.gz",
          "type": "tar"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "androguard/androguard",
        "version": "3.1.0",
        "source": {
          "url": "https://github.com/androguard/androguard.git",
          "type": "git",
          "reference": "v3.1.0"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "ExodusFdroid\\": "classes/"
    }
  },
  "bin": [
    "exodus-fdroid"
  ],
  "scripts": {
    "test": "phpunit"
  }
}
